<package xmlns="http://schemas.microsoft.com/packaging/2011/08/nuspec.xsd">
    <metadata>
        <id>TeamCityAgent</id>
        <version>2.0.1-beta-03</version>
        <title>TeamCity Build Agent</title>
        <authors>JetBrains</authors>
        <owners>maartenba;jetbrains</owners>
        <projectUrl>http://www.jetbrains.com/teamcity</projectUrl>
        <packageSourceUrl>https://github.com/maartenba/ChocolateyPackages/tree/master/packages/TeamCityAgent</packageSourceUrl>
